{
  "name": "quick-typescript-starter",
  "version": "0.0.3",
  "description": "Typescript project starter command line tool",
  "main": "starter.js",
  "homepage": "https://github.com/Doth-J/quick-typescript-starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/Doth-J/quick-typescript-starter"
  },
  "scripts": {
    "test": "ts-node src/starter.ts",
    "build":"tsc",
    "refresh":"npm uninstall -g typescript-starter && npm i -g",
    "site":"npm run build && npm run refresh"
  },
  "bin": {
    "ts-start":"build/starter.js"
  },
  "keywords": [
    "typescript",
    "starter",
    "commander"
  ],
  "author": "Doth-J",
  "license": "MIT",
  "dependencies": {
    "commander": "^10.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
